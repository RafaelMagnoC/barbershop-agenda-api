model User {
  id         String    @id @default(uuid())
  name       String
  birthday   DateTime
  gender     Gender
  username   String?   @unique
  is_active  Boolean   @default(true)
  token      String?   @unique
  created_at DateTime  @default(now())
  updated_at DateTime?

  // relations
  address  Address?  @relation
  contact  Contact?  @relation
  document Document? @relation

  // relations user rules
  administrator    Administrator? @relation(fields: [administrator_id], references: [id], onDelete: Cascade)
  administrator_id String         @unique
  receptionist     Receptionist?  @relation
  develop          Develop?       @relation
  attendant        Attendant?     @relation
  client           Client?        @relation

  // relation with enterprise
  enterprises UserEnterprise[]

  @@map("user")
}

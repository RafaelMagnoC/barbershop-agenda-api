model User {
  id              String             @id @default(uuid())
  name            String
  birthday DateTime
  username String @unique
  isActive Boolean
  token String? @unique
    

  //relations
  address         Address?        @relation(fields: [addressId], references: [id])
  addressId       String? @unique
  contact         Contact?        @relation(fields: [contactId], references: [id])
  contactId       String? @unique
  registerDocument RegisterDocument? @relation(fields: [registerDocumentId], references: [id])
  registerDocumentId String? @unique

  
  //map
  @@map("users")
}
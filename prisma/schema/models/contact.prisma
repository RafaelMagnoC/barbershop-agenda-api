model Contact {
  id              String             @id @default(uuid())
  phone     String? @unique
  whatsapp String? @unique
  email     String? @unique
  facebook String? @unique
  instagram String? @unique
  created_at DateTime? @default(now())
  updated_at DateTime? 


  //relations
  user      User?  @relation

  //maps
  @@map("contacts") // Optional: Maps the model to a different table name
}